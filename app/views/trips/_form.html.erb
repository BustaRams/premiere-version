<%= simple_form_for @trip, :html => { :multipart => true } do |f| %>
	<%= select_tag(:category_id, options_for_select(@categories), :prompt => "Select a Category") %>
	<%= f.input :title, label: "Trip Title" %>
	<%= f.input :description %>
	<%= f.input :language %>
	<%= f.input :start %>
	<%= f.button :submit %>

<% end %>